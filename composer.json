{
    "name": "thetrulymadly/coronaclusters",
    "type": "project",
    "description": "Analysis and visualization of crowdsourced data on COVID19 (Laravel-PHP App)",
    "keywords": [
        "covid19",
        "covid-19",
        "covid-data",
        "covid-api",
        "covid-19-india",
        "coronavirus",
        "coronavirus-tracker",
        "coronavirus-tracking",
        "coronavirus-analysis",
        "coronavirus-real-time",
        "coronavirus-info",
        "coronavirus-india",
        "corona",
        "coronavirus-api",
        "covid19-data",
        "covid19-india",
        "covid19-tracker",
        "covid19-live-tracker",
        "covid19-app"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "bensampo/laravel-enum": "^1.0",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.5",
        "joedixon/laravel-translation": "^1.1",
        "laravel/framework": "^6.2",
        "laravel/telescope": "^3.2",
        "laravel/tinker": "^2.6",
        "laravelcollective/html": "^6.2",
        "renatomarinho/laravel-page-speed": "^2.0",
        "silber/page-cache": "^1.0"
    },
    "require-dev": {
        "arubacao/asset-cdn": "^0.2.4",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.9.1",
        "laravel/ui": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "spatie/laravel-sitemap": "^5.8"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Api\\": "api/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
